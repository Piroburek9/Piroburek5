{
  "name": "eduplatform",
  "version": "1.0.0",
  "description": "Modern educational platform with testing, AI assistant, and progress tracking",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "vite --host 0.0.0.0 --port 3000",
    "dev:backend": "cd server && npm install && npm run dev",
    "dev:deno": "deno run --allow-net --allow-env --allow-read --allow-write --watch supabase/functions/server/index.tsx",
    "build": "vite build",
    "build:backend": "cd server && npm run build",
    "preview": "vite preview",
    "start": "npm run build && npm run preview",
    "start:backend": "cd server && npm start",
    "start:full": "concurrently \"npm run start:backend\" \"npm run preview\"",
    "backend:init": "curl -X POST http://localhost:8000/api/init",
    "backend:install": "cd server && npm install",
    "test": "echo \"Add tests later\" && exit 0",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-aspect-ratio": "^1.1.7",

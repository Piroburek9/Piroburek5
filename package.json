    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.263.1",
    "motion": "^10.16.2",
    "pdfjs-dist": "^4.4.168",
    "react": "^18.2.0",
    "react-day-picker": "^9.9.0",
